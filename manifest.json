{
  "manifest_version": 2,

  "name": "gifthis",
  "description": "Replace images with gifs",
  "version": "1.0",

  "permissions": [
    "http://api.giphy.com"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
      "js": ["jquery-1.11.1.min.js", "alert.js"],
      "css": ["style.css"],
      "matches": [ "http://*/*", "https://*/*"]
    }]
}